<application>
  <component name="FileTypeManager" version="17">
    <ignoreFiles list="*.hprof;*.pyc;*.pyo;*.rbc;*~;.DS_Store;.git;.hg;.svn;CVS;RCS;SCCS;__pycache__;_svn;rcs;" />
    <extensionMap>
      <mapping pattern=".php_cs" type="PHP" />
      <mapping pattern=".htpasswd" type="PLAIN_TEXT" />
      <mapping pattern=".gitkeep" type="PLAIN_TEXT" />
      <mapping pattern=".htacces" type="PLAIN_TEXT" />
      <mapping pattern=".gitattributes" type="PLAIN_TEXT" />
      <mapping ext="excludes" type="XML" />
    </extensionMap>
  </component>
</application>